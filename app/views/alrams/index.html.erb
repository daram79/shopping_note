<h1>Listing alrams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alrams.each do |alram| %>
      <tr>
        <td><%= link_to 'Show', alram %></td>
        <td><%= link_to 'Edit', edit_alram_path(alram) %></td>
        <td><%= link_to 'Destroy', alram, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alram', new_alram_path %>
